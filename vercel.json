{
  "version": 2,
  "builds": [
    {
      "src": "new.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/products",

      "dest": "new.js"
    },
    {
      "src": "/products/(.*)",

      "dest": "new.js"
    },
    {
      "src": "/",

      "dest": "public/index.html"
    },
    {
      "src": "/(.+)",

      "dest": "dist/$1"
    }
  ]
}
